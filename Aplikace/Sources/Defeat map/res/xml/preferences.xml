<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >
    
    <CheckBoxPreference android:key="gps_checkstartup" 
        android:title="@string/pref_check_gps_startup" 
        android:defaultValue="true" 
        android:summaryOn="@string/pref_check_gps_startup_summary_en"
        android:summaryOff="@string/pref_check_gps_startup_summary_dis"  />
    
    <EditTextPreference android:key="gps_interval"
        android:title="@string/pref_gps_interval"
        android:inputType="number"
        android:defaultValue="5" />
    
    
    
    <CheckBoxPreference android:key="gui_keep_screen_on" 
        android:summary="@string/pref_keep_screen_summary" 
        android:title="@string/pref_keep_screen" 
        android:defaultValue="true" />
    
    <Preference android:key="about"
        android:title="@string/pref_about" />
    
    
    

</PreferenceScreen>